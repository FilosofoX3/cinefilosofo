<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.ClassificacaoEntity" table="Classificacao" schema="cinema">
        <id name="classificacaoId">
            <column name="classificacao_id" sql-type="mediumint(9)"/>
        </id>
        <property name="nome">
            <column name="nome" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>