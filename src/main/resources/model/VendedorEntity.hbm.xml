<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.VendedorEntity" table="Vendedor" schema="cinema">
        <id name="vendedorId">
            <column name="vendedor_id" sql-type="mediumint(9)"/>
        </id>
        <property name="usuarioId">
            <column name="usuario_id" sql-type="varchar(35)" length="35" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>