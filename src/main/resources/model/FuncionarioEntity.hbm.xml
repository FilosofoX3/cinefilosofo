<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.FuncionarioEntity" table="Funcionario" schema="cinema">
        <id name="funcionarioId">
            <column name="funcionario_id" sql-type="mediumint(9)"/>
        </id>
        <property name="codigo">
            <column name="codigo" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="nome">
            <column name="nome" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="dataCadastro">
            <column name="data_cadastro" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>